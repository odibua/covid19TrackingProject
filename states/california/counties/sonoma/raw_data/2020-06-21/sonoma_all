	        <script>
/**
* Function that registers a click on an outbound link in Analytics.
* This function takes a valid URL string as an argument, and uses that URL string
* as the event label. Setting the transport method to 'beacon' lets the hit be sent
* using 'navigator.sendBeacon' in browser that support it.
* Example:
* Add .gtag-outbound-link class to link
* <a class="gtag-outbound-link" href="http://www.example.com">Check out example.com</a>
*/
window.onload = function() {addOutboundLinkAction()};

function addOutboundLinkAction() {
	var links =  document.querySelectorAll('.gtag-outbound-link');
	
	[].forEach.call(links, function(link) {
		link.addEventListener("click", function(){
			getOutboundLink(link.href)
		});
	});
}

var getOutboundLink = function(url) {
gtag('event', 'click', {
	'event_category': 'outbound',
	'event_label': url,
	'transport_type': 'beacon',
	'event_callback': function(){document.location = url;}
});
}

/**
 * Change the Esri Dashboard if user is on mobile
 */

if (screen.width <= 699) {
	window.onload = function() {switchLinksOnMobile()};

	function switchLinksOnMobile() {
		// OLD Eng
		var links =  document.querySelectorAll('a[href="https://sonomacounty.maps.arcgis.com/apps/MapSeries/index.html?appid=21a1653b79ba42039ff22bcb85fa5b19"]');
		[].forEach.call(links, function(link) {
			link.setAttribute('href', 'https://experience.arcgis.com/experience/c792b1ddd7f84914a4c7b9ab3396280a')
		});

		// OLD Spanish
		links =  document.querySelectorAll('a[href="https://sonomacounty.maps.arcgis.com/apps/MapSeries/index.html?appid=8e3a824b1446473280208028a794b03f"]');
		[].forEach.call(links, function(link) {
			link.setAttribute('href', 'https://experience.arcgis.com/experience/eedfbf900d7a4c608936d690f535f7dc')
		});

		// New Eng
		links =  document.querySelectorAll('a[href="https://sonomacounty.ca.gov/coronavirus-storymap-desktop"]');
		[].forEach.call(links, function(link) {
			link.setAttribute('href', 'https://sonomacounty.ca.gov/coronavirus-storymap-mobile')
		});

		// New Eng
		links =  document.querySelectorAll('a[href="https://sonomacounty.ca.gov/Novel-Coronavirus-Storymap/"]');
		[].forEach.call(links, function(link) {
			link.setAttribute('href', 'https://sonomacounty.ca.gov/coronavirus-storymap-mobile')
		});

		// New Spanish
		links =  document.querySelectorAll('a[href="https://sonomacounty.ca.gov/coronavirus-mapahistorial-escritorio"]');
		[].forEach.call(links, function(link) {
			link.setAttribute('href', 'https://sonomacounty.ca.gov/coronavirus-mapahistorial-movil')
		});

		// New Spanish 2
		links =  document.querySelectorAll('a[href="https://sonomacounty.ca.gov/coronavirus-storymap-desktop-es"]');
		[].forEach.call(links, function(link) {
			link.setAttribute('href', 'https://sonomacounty.ca.gov/coronavirus-storymap-mobile-es')
		});
	}
}
</script>
